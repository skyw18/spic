    <system:String x:Key="WOpenFile">Open File</system:String>
    <system:String x:Key="WClose">Close</system:String>
    <system:String x:Key="WHelp">Help</system:String>
    <system:String x:Key="WSetting">Setting</system:String>
    <system:String x:Key="WPicinfo">Picture Information</system:String>
